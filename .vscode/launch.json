{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug shapes_lines_cpp",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/examples/cpp/shapes_lines",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/examples/cpp",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "make_cpp_examples"
      }
    ]
  }
  